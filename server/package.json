{
  "name": "video-server",
  "version": "0.0.1",
  "license": "MIT",  
  "description": "Video Aulas!",
  "main": "index.js",
  "engines" : {
    "node" : ">=8.5.0",
    "npm" : ">=3.5.0"
  },
  
  "scripts": {
    "dev":          "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build":        "babel src -s -D -d build --presets es2015,stage-0",
    "start":        "pm2 start ecosystem.config.js --only app",
    "restart":      "pm2 restart ecosystem.config.js --only app",
    "stop":         "pm2 stop ecosystem.config.js --only app",
    "reload":       "pm2 reload ecosystem.config.js --only app",
    "logs":         "pm2 logs app",
    "show":         "pm2 show app",
    "prestart_old": "yarn run -s build",
    "test":         "eslint src"
  },
  "dependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "bcrypt": "^1.0.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "debug": "^2.2.0",
    "deep-freeze": "^0.0.1",
    "express": "^4.15.2",
    "express-limiter": "^1.6.0",
    "file-loader": "^0.10.1",
    "fs-extra": "^3.0.1",
    "global": "^4.3.1",
    "immutable": "^3.8.1",
    "imports-loader": "^0.7.1",
    "json-loader": "^0.5.4",
    "jsonwebtoken": "^7.3.0",
    "mongoose": "^4.8.4",
    "nodemailer": "^3.1.3",
    "nodemon": "^1.10.2",
    "normalize.css": "^5.0.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "redis": "^2.7.1",
    "request": "^2.81.0",
    "request-promise": "^4.2.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8"
  }
}
